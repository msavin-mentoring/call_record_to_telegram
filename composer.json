{
  "name": "savinmikhail/call-record-to-telegram",
  "type": "project",
  "autoload": {
    "psr-4": {
      "App\\": "src/App/"
    }
  },
  "require": {
    "vlucas/phpdotenv": "^5.6",
    "guzzlehttp/guzzle": "^7.9",
    "monolog/monolog": "^3.8"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "rector/rector": "^2.3",
    "savinmikhail/add_named_arguments_rector": "^0.1.22",
    "phpmd/phpmd": "^2.15"
  },
  "scripts": {
    "phpstan": "phpstan analyse -c phpstan.neon --memory-limit=1G",
    "rector": "rector process -c rector.php",
    "rector:dry-run": "rector process -c rector.php --dry-run"
  }
}
